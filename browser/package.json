{
  "name": "@cerealia/webapp",
  "version": "0.0.1",
  "description": "Cerealia web application (client)",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "antd": "^3.7.3",
    "apollo-boost": "^0.3.1",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "blake2s": "^1.1.0",
    "chalk": "~1.1.3",
    "dotenv-expand": "~4.2.0",
    "file-loader": "1.1.5",
    "global": "^4.3.2",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "html-webpack-externals-plugin": "^3.8.0",
    "intl": "^1.2.5",
    "marked": "^0.4.0",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "mobx-react-lite": "^1.0.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "react": "^16.8.2",
    "react-apollo": "^2.5.5",
    "react-collapse": "^4.0.3",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.8.2",
    "react-drag-sortable": "^1.0.6",
    "react-dropzone": "^4.2.13",
    "react-intl": "^2.4.0",
    "react-motion": "^0.5.2",
    "react-router": "^4.4.0-beta.6",
    "react-router-dom": "^4.4.0-beta.6",
    "react-timezone": "^2.1.0",
    "resolve": "1.6.0",
    "stellar-base": "latest",
    "stellar-sdk": "^0.11.0",
    "url-loader": "0.6.2",
    "use-react-router": "^1.0.3"
  },
  "scripts": {
    "start": "REACT_APP_GIT_BRANCH=`git rev-parse --abbrev-ref HEAD` REACT_APP_GIT_SHA=`git rev-parse --short HEAD` node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "eslint --ext .js --ext .jsx src",
    "lint-fix": "eslint --fix --ext .js --ext .jsx src",
    "test": "node scripts/test.js --env=jsdom",
    "test:all": "yarn run lint && flow && node scripts/test.js --env=jsdom --coverage",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/cerealia/apps.git"
  },
  "author": "Cerealia Team",
  "license": "SEE LICENSE IN /LICENSE",
  "homepage": "https://cerealia.ch",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "apollo-link-schema": "^1.1.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "^7.0.1",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.7",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-react-hooks": "^1.5.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "flow-bin": "^0.95.1",
    "fs-extra": "3.0.1",
    "graphql-tools": "^4.0.4",
    "html-webpack-plugin": "2.29.0",
    "jest": "24.1.0",
    "mobx-react-devtools": "^6.0.3",
    "node-sass": "^4.9.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.5",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/*.(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ]
  }
}
